int num = int.Parse(Console.ReadLine());
int startNum = int.Parse(Console.ReadLine());
int sum = 0;

if (startNum > 10)
{
    sum = num * startNum;
    Console.WriteLine($"{num} X {startNum} = {sum}");
}
else
{
    for (int i = startNum; i <= 10; i++)
    {
        sum = num * i;
        Console.WriteLine($"{num} X {i} = {sum}");
    }
}
